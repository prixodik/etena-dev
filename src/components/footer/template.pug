
mixin footer(data)
	footer.footer.js-footer&attributes(attributes)
		._shadow-1
		._logo.js-footer-logo
			img.js-scale(src="images/footer/footer-logo.svg", alt="")
		._mountain
		a._up.js-scroll-up.js-scale(href="#")
			._up-pixel-left
				img.js-dotted(data-size="10" src="images/footer/footer-button-pixel-left.svg", alt="")
			._up-pixel-right
				img.js-dotted(data-size="10" src="images/footer/footer-button-pixel-right.svg", alt="")
			img._up-arrow(src="images/footer/footer-button-up-arrow.webp", alt="")
			img(src="images/footer/footer-button-up.webp", alt="")
		._shadow-2
		img._mountain-front(src="images/footer/footer-mountain-front.webp", alt="")
		._hero.js-footer-hero
			img.js-scale(src="images/footer/footer-hero.webp", alt="")
		._container.-min.js-footer-text
			._title.title-h1 Begin Your Adventure in the World of Etena.
			._buttons 
				a.btn(href="#")
					span Inferno magic travel
			
		._bottom
			._container 
				._copyright All Rights Reserved Â© ETENAlaB